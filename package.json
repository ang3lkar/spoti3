{
  "name": "spoti3",
  "version": "1.7.2",
  "description": "A simple CLI tool to download songs from Spotify playlists to MP3 format using YouTube",
  "main": "index.js",
  "type": "module",
  "bin": {
    "spoti3": "./src/cli/index.js"
  },
  "scripts": {
    "test": "NODE_ENV=test node --test",
    "start": "node src/cli/index.js mp3",
    "debug": "LOG_LEVEL=debug MOCK_DOWNLOAD=yes node --inspect src/cli/index.js mp3",
    "lint": "eslint ."
  },
  "keywords": [
    "spotify",
    "playlist",
    "download",
    "mp3",
    "youtube",
    "music",
    "cli",
    "downloader"
  ],
  "author": "Angelos Karagiozidis",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ang3lkar/spoti3.git"
  },
  "homepage": "https://github.com/ang3lkar/spoti3",
  "bugs": {
    "url": "https://github.com/ang3lkar/spoti3/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "1.7.4",
    "chalk": "5.4.1",
    "commander": "12.1.0",
    "consola": "3.2.3",
    "dotenv": "16.4.5",
    "get-artist-title": "1.3.1",
    "metadata-filter": "1.3.0",
    "mp3tag.js": "3.11.0"
  },
  "devDependencies": {
    "eslint": "9.39.2",
    "globals": "16.5.0"
  }
}
